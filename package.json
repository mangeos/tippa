{
  "name": "tippa",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:postcss": "set NODE_ENV=production && postcss src/main/resources/static/css/style.tailwind.css -o target/classes/static/css/style.build.tailwind.css",
    "build-linux:postcss": "NODE_ENV=production postcss src/main/resources/static/css/style.tailwind.css -o target/classes/static/css/style.build.tailwind.css",
    "watch:postcss": "nodemon --watch 'src/**/*.html' --exec 'npm run build:postcss' --ext 'html,css'",
    "watch-linux:postcss": "nodemon --watch 'src/**/*.html' --exec 'npm run build-linux:postcss' --ext 'html,css'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.3"
  }
}
